<h1> Edit Post </h1>

<% if flash[:errors] %>
   <% flash[:errors].each do |error| %>
    <%=    error %>
    <% end %> 
<% end %>

<%= form_for @post do |f| %>
<%= f.label :title %>
<%= f.text_field :title %> <br>
<%= f.label :content %>
<%= f.text_area :content %><br>
<%= f.label :age %>
<%= f.number_field :age %>
<%= f.submit %>
<% end %>